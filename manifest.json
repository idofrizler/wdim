{
    "manifest_version": 3,
    "name": "What Did I Miss?",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://web.whatsapp.com/*"],
            "js": ["content.js"]
        }
    ]
}
